{
  "name": "thought",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "date": {
      "type": "date",
      "required": true
    },
    "where": {
      "type": "string",
      "required": true
    },
    "what": {
      "type": "string",
      "required": true
    },
    "expectation": {
      "type": "string",
      "required": true
    },
    "reality": {
      "type": "string",
      "required": true
    },
    "strengthInitial": {
      "type": "number"
    },
    "automaticThought": {
      "type": "string"
    },
    "supportingEvidence": {
      "type": "string"
    },
    "nonSupportingEvidence": {
      "type": "string"
    },
    "thinkingErrors": {
      "type": "string"
    },
    "newBelief": {
      "type": "string"
    },
    "strengthFinal": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": ""
    },
    "negative-feelings": {
      "type": "hasMany",
      "model": "negative-feeling",
      "foreignKey": "negativeFeelingId",
      "primaryKey": "id",
      "options": {
        "disableInclude": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "*"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
